{
  "name": "doc-reader",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "express": "^5.1.0",
    "multer": "^2.0.2",
    "prisma": "^6.19.0",
  "zod": "^4.1.12"
  },
  "devDependencies": {
    "dotenv": "^17.2.3",
    "jest": "^30.2.0",
    "supertest": "^7.1.4",
    "nodemon": "^3.0.1"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.test.[jt]s"
    ],
    "setupFilesAfterEnv": ["<rootDir>/tests/setup/setup.js"]
  }
  ,
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watchAll",
    "test:watch:nodemon": "NODE_ENV=test nodemon --watch src --watch tests --ext js,json --exec \"npm test\"",
    "prisma": "prisma",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev --schema=src/prisma/schema.prisma",
    "prisma:migrate:test": "NODE_ENV=test prisma migrate deploy --schema=src/prisma/schema.prisma",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "test:watch:nodemon:coverage": "NODE_ENV=test nodemon --watch src --watch tests --ext js,json --exec \"npm run test:coverage\""
  }
}
